{
    "name":"Serotonin",
    "version":"1.0",
    "description":"Improving Mental Health One Step at a Time",
    "manifest_version":2,
    "icons": { "16" : "images/logo.png",
               "48" : "images/logo.png",
               "128" : "images/logo.png"},
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action":{
      "default_icon": "images/logo.png",
      "default_popup":"popup.html"
    },
    "content_security_policy": "script-src 'self' https://canvasjs.com/assets/script/canvasjs.min.js; object-src 'self'"
  }
